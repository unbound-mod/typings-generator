{
  "compilerOptions": {
    "downlevelIteration": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "target": "es2022",
    "resolveJsonModule": true,
    "module": "es2022",
    "jsx": "react",
    "baseUrl": "src",
    "paths": {
      "@util/*": [
        "util/*"
      ],
      "@instances/*": [
        "instances/*"
      ],
      "@visitors/*": [
        "visitors/*"
      ],
      "@visitors": [
        "visitors/index.ts"
      ]
    }
  }
}